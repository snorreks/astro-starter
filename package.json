    "astro": "^5.16.2",
    "astro-compress": "^2.3.8",
    "astro-seo-plugin": "^1.3.5",
    "daisyui": "^5.5.5",
    "type-fest": "^5.2.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@axe-core/playwright": "^4.11.0",

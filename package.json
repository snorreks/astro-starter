    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "astro-eslint-parser": "^1.2.2",
    "astro-robots-txt": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^61.4.1",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-no-use-extend-native": "^0.7.2",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "firebase-tools": "^14.26.0",
    "globals": "^16.5.0",
    "jsonc-eslint-parser": "^2.4.1",
    "knip": "^5.70.2",
    "lefthook": "^2.0.4",
    "lint-staged": "^16.2.7",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.7.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript-eslint": "^8.48.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "name": "astro-starter",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "lint": "eslint . --flag unstable_native_nodejs_ts_config",
    "format": "prettier --write .",
    "test:e2e": "playwright test",
    "generate": "tsx scripts/generate-public-assets.ts",
    "deploy": "firebase deploy --only hosting",
    "fix": "pnpm format && pnpm lint --fix && pnpm check",
    "fix:changed": "tsx scripts/fix-changed.ts",
    "validate": "prettier --check . && eslint . --flag unstable_native_nodejs_ts_config && astro check",
    "postinstall": "lefthook install"
  },
  "type": "module",
  "version": "0.0.1"
}
